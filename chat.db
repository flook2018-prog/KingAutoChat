CREATE TABLE IF NOT EXISTS clients (
    userId TEXT PRIMARY KEY,
    userName TEXT,
    oaName TEXT,
    assignedAdmin TEXT,
    username TEXT,
    regDate TEXT,
    note TEXT
);

CREATE TABLE IF NOT EXISTS messages (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    userId TEXT,
    sender TEXT,
    text TEXT,
    time INTEGER,
    FOREIGN KEY(userId) REFERENCES clients(userId)
);
